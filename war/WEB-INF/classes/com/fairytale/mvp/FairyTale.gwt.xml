<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='FairyTale'>
	<inherits name='com.google.gwt.user.User' />
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>
    
	<entry-point class='com.fairytale.mvp.client.FairyTaleEntryPoint' />
    
    <replace-with class="com.fairytale.mvp.client.ClientFactoryImpl">
        <when-type-is class="com.fairytale.mvp.client.ClientFactory" />
    </replace-with>
    
	<source path='client' />
</module>